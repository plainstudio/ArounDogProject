        <mapper resource="com/aroundog/mybatis/mapper/FreeCommentMapper.xml"/>
        <mapper resource="com/aroundog/mybatis/mapper/MemberMapper.xml" />
        <mapper resource="com/aroundog/mybatis/mapper/TypeMapper.xml"/>
        <mapper resource="com/aroundog/mybatis/mapper/AdoptdogMapper.xml"/>
        <mapper resource="com/aroundog/mybatis/mapper/AdoptboardMapper.xml"/>
        <mapper resource="com/aroundog/mybatis/mapper/LostBoardMapper.xml"/>
        <mapper resource="com/aroundog/mybatis/mapper/LostBoardImgMapper.xml"/>
        <mapper resource="com/aroundog/mybatis/mapper/LostCommentMapper.xml"/>
        <mapper resource="com/aroundog/mybatis/mapper/NoticeMapper.xml"/>
