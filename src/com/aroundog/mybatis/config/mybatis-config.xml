<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.aroundog.model.domain.Admin" alias="Admin"/>
		<typeAlias type="com.aroundog.model.domain.FreeBoard" alias="FreeBoard"/>
		<typeAlias type="com.aroundog.model.domain.Adopt" alias="Adopt"/>
		<typeAlias type="com.aroundog.model.domain.Report" alias="Report"/>
